{
  "name": "spec-workflow",
  "version": "0.1.0",
  "description": "AI-powered specification generation workflow using multi-agent consensus",
  "type": "module",
  "main": "dist/council.js",
  "scripts": {
    "build": "tsc",
    "council": "node dist/council.js",
    "phase": "node dist/phase.js",
    "test-council": "node dist/test-council.js",
    "test-finalize": "node dist/test-finalize.js",
    "finalize": "node dist/finalize.js",
    "init": "node dist/init.js",
    "validate": "node dist/validate.js",
    "export:spec": "node dist/export-spec.js",
    "export:tests": "node dist/export-tests.js",
    "export:all": "npm run export:spec && npm run export:tests",
    "traceability": "node dist/traceability.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:contract": "vitest run tests/contract",
    "test:smoke": "vitest run tests/smoke",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "agent-council": "github:bladehstream/agent-council#main"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "typescript": "^5.0.0",
    "vitest": "^4.0.16"
  }
}
