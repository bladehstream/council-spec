{
  "name": "spec-workflow",
  "version": "0.1.0",
  "description": "AI-powered specification generation workflow using multi-agent consensus",
  "type": "module",
  "main": "dist/council.js",
  "scripts": {
    "build": "tsc",
    "council": "node dist/council.js",
    "finalize": "node dist/finalize.js",
    "init": "node dist/init.js",
    "validate": "node dist/validate.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:contract": "vitest run tests/contract",
    "test:smoke": "vitest run tests/smoke",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "agent-council": "file:../agent-council/repo"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "typescript": "^5.0.0",
    "vitest": "^4.0.16"
  }
}
